{
  "$schema": "https://raw.githubusercontent.com/tauri-apps/tauri/v2.0.1/tooling/cli/schema.json",
  "identifier": "com.ultrasearch.app",
  "productName": "UltraSearch",
  "version": "0.1.0",
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 800,
        "resizable": true,
        "title": "UltraSearch",
        "width": 1100,
        "decorations": true,
        "transparent": false
      }
    ]
  },
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../frontend/dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "icon": [
      "icons/icon.ico"
    ],
    "targets": "all",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}
